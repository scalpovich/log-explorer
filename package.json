{
  "name": "log-explorer",
  "version": "0.1.1",
  "private": true,
  "dependencies": {
    "electron-is-dev": "^1.0.1",
    "electron-localshortcut": "^3.1.0",
    "react": "^16.6.1",
    "react-beautiful-dnd": "^9.0.2",
    "react-dom": "^16.6.1",
    "react-dropdown": "^1.6.2",
    "react-modal": "^3.6.1",
    "react-scripts": "2.1.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron-dev": "concurrently \"BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron-pack": "build -c.extraMetadata.main=build/electron.js --win --mac --publish=always",
    "preelectron-pack": "yarn build",
    "electron-pack-test": "build -c.extraMetadata.main=build/electron.js",
    "preelectron-pack-test": "yarn build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "main": "public/electron.js",
  "homepage": "./",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "concurrently": "^4.0.1",
    "electron": "^3.0.8",
    "electron-builder": "^20.33.2",
    "wait-on": "^3.2.0"
  },
  "build": {
    "productName": "Log Explorer",
    "appId": "com.innovationz-unlimited.log-explorer",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "fileAssociations": [
      {
        "ext": "txt",
        "role": "Editor"
      },
      {
        "ext": "log",
        "role": "Editor"
      }
    ],
    "mac": {
      "category": "public.app-category.developer-tools",
      "icon": "build/icon.png",
      "publish": [
        "github"
      ]
    },
    "win": {
      "icon": "build/icon.png",
      "publish": [
        "github"
      ]
    },
    "publish": [
      {
        "provider": "github",
        "repo": "log-explorer",
        "owner": "navedr",
        "releaseType": "release"
      }
    ]
  },
  "author": "Naved Rangwala"
}
